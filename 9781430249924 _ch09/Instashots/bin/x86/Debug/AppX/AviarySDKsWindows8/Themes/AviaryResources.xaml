    <Color x:Key="AviaryRibbonBackgroundColor">#FF000000</Color>
    <SolidColorBrush x:Key="AviaryRibbonBackgroundBrush" Color="{StaticResource AviaryRibbonBackgroundColor}" />
    <Color x:Key="AviaryRibbonBackgroundComplementColor">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="AviaryRibbonBackgroundComplementBrush" Color="{StaticResource AviaryRibbonBackgroundComplementColor}" />

    <Color x:Key="AviaryBlueColor">#FF2DC7FF</Color>
    <SolidColorBrush x:Key="AviaryBlueBrush" Color="{StaticResource AviaryBlueColor}" />
    <Color x:Key="AviaryBlackColor">#FF363636</Color>
    <SolidColorBrush x:Key="AviaryBlackBrush" Color="{StaticResource AviaryBlackColor}" />
    <Color x:Key="AviaryRibbonItemSelectedColor">#30FFFFFF</Color>
    <SolidColorBrush x:Key="AviaryRibbonItemSelectedBrush" Color="{StaticResource AviaryRibbonItemSelectedColor}" />

    <Color x:Key="AviaryTextControlBorderColor">#B34D4D4D</Color>
    <SolidColorBrush x:Key="AviaryTextControlBorderBrush" Color="{StaticResource AviaryTextControlBorderColor}" />

    <Color x:Key="AviaryTextControlBackgroundColor">#3EFFFFFF</Color>
    <SolidColorBrush x:Key="AviaryTextControlBackgroundBrush" Color="{StaticResource AviaryTextControlBackgroundColor}" />

